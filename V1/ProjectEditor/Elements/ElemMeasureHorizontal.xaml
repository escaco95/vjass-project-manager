<UserControl x:Class="vJassMainJBlueprint.V1.ProjectEditor.Elements.ElemMeasureHorizontal"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:vJassMainJBlueprint.V1.ProjectEditor.Elements"
             mc:Ignorable="d" 
             d:DesignHeight="500" d:DesignWidth="500">
    <Border>
        <Grid VerticalAlignment="Top" Height="100" Margin="0,-100,0,0" HorizontalAlignment="Center">
            <TextBlock Text="{Binding Text, RelativeSource={RelativeSource AncestorType=UserControl}}" FontSize="50" Foreground="White" TextAlignment="Center" VerticalAlignment="Center"/>
        </Grid>
    </Border>
</UserControl>
